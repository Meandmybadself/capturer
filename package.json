{
  "name": "react-ts-scss-boilerplate",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@8.15.0",
  "type": "module",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "xo": {
    "extends": [
      "prettier"
    ],
    "ignore": [
      "node_modules/**/*.*"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-call": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "npm run lint",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "xo": "^0.60.0"
  }
}